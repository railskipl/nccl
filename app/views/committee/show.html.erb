<% content_for :styles do %>
 <%= stylesheet_link_tag "table"%> 
<% end %>

<!-- content -->
    <div id="content">

      <div class="indent">
        <div class="wrapper">
          <div class="mainContent">
		  

              <div class="wrapper">
           
              <h2><%= @committee_type.title %></h2>
                 
                    <div class="extra-wrap">
  <table width="100%" border="0" cellspacing="5" cellpadding="5">
  <tr>
    <td align="left" valign="top"><strong>S.No </strong></td>
    <td align="left" valign="top"><strong>Photo</strong></td>
    <td align="left" valign="top"><strong>NAME OF DIRECTORS</strong></td>
    <td align="left" valign="top"><strong>POST</strong></td>
    <td align="left" valign="top"><strong>PH. No.</strong></td>
  </tr>
  <% members = Committee.find_all_by_committee_type_id(@committee_type.id, :order=>"id asc").paginate(:per_page => 6, :page => params[:page]) %>
  <% members.each do |member| %>
  <tr>
    <td align="left" valign="top"><%= member.id %></td>
    <td align="left" valign="top">
       
      <% if member.photo.url(:thumb) == "/photos/thumb/missing.png" %> 
      		<img src="/assets/avatar.jpg" alt="" width="100" height="100" class="avatar" />
      <% else %>
      		<%= image_tag member.photo.url(:thumb), :class=>"avatar" %>
      <% end %>
    
    </td>
    <td align="left" valign="top"><strong><%= member.name %></strong><br />
      <%= member.address.html_safe %>
    <td align="left" valign="top"><strong><%= member.designation %></strong></td>
    <td align="left" valign="top"><p><%= member.contact_info.html_safe %></td>
  </tr>
 
  <% end %>
    
    
</table>
      <p><%= will_paginate members, :class=>'pagination_magArticles' %></p>

                    </div>
               
               
              </div>
           
            
          </div>
          <div class="aside">
		 
		  
		  
		  
		      

          <%= render "event_press" %>
            

          </div>
        </div>
      </div>
    </div>