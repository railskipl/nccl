 

 <div id="content">
 	
		<p class='clear'></p>
		   

			<table class="admintable">
			  <tr class="admintablehd">
			    <td width="25%">Title</td>
			    <td>Description</td>
			    <td width="27%" style="text-align: center !important;">Action</td>
			  </tr>
			
			<% @contacts.each do |contact| %>
			  <tr>
			    <td><%= contact.name %></td>
			    <td><%= contact.subject %> </td>
			    <td style="text-align: center !important;"> <%= link_to "Show", admin_contact_path(contact), :class=>'button positive' %> &nbsp;&nbsp; 
				<%= link_to "Delete", admin_contact_path(contact), :method=> :delete , :confirm => "Are you sure?",  :class=>'button negative' %>
				</td>
			    
			  </tr>
			<% end %>
			</table>
			
			<br />
</div>