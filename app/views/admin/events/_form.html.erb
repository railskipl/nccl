



<%= form_for [:admin, @event],:html => { :multipart => true } do |f| %>

  
  <%- @event.errors.full_messages.each do |message| -%>
   <div class="errorExplanation"> <p><%= message %></p></div>
  <%- end -%>
   
  
  <div id="breadcrumb">
  	   <%= link_to "Event", admin_events_path %> &raquo; New
  </div>
 
  <p>
    <%= f.label :name, "Event Title" %>
    <%= f.text_field :name, :class => 'text' %>
  </p>
  
  <%= render :partial =>"shared/button" %>

<% end %>


