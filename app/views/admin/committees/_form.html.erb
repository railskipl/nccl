



<%= form_for [:admin, :committee_type, @committee],:html => { :multipart => true } do |f| %>

  
  <%- @committee.errors.full_messages.each do |message| -%>
   <div class="errorExplanation"> <p><%= message %></p></div>
  <%- end -%>
   
  
  <div id="breadcrumb">
  	   <%= link_to "Member list", admin_committee_type_committees_path(@committee_type_id) %> &raquo; New
  </div>
 
  <p>
    <%= f.label :name, "Full Name" %>
    <%= f.text_field :name, :class => 'text' %>
  </p>
  
  <p>
    <%= f.label :address %>
    <%= f.text_area :address, :class => 'ckeditor' %>
  </p>
  
  <p>
    <%= f.label :contact_info %>
    <%= f.text_area :contact_info, :class => 'ckeditor' %>
  </p>
  
  <p>
    <%= f.label :education %>
    <%= f.text_field :education, :class => 'text' %>
  </p>
  
  <p>
    <%= f.label :designation %>
    <%= f.text_field :designation, :class => 'text' %>
  </p>
  
  <p>
    <%= f.label :photo %>
    <%= f.file_field :photo %>
  </p>

  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <%= f.hidden_field :committee_type_id, :value=> "#{@committee_type_id.id}" %>
  </p>
  
  
  <%= render :partial =>"shared/button" %>

<% end %>


