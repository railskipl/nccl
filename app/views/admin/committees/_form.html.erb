



<%= form_for [:admin, @committee],:html => { :multipart => true } do |f| %>

  
  <%- @committee.errors.full_messages.each do |message| -%>
   <div class="errorExplanation"> <p><%= message %></p></div>
  <%- end -%>
   
  
  <div id="breadcrumb">
  	   <%= link_to "Pages", admin_pages_path %> &raquo; New
  </div>
 
  <p>
    <%= f.label :name, "Full Name" %>
    <%= f.text_field :name, :class => 'text' %>
  </p>
  
  <p>
    <%= f.label :education %>
    <%= f.text_field :education, :class => 'text' %>
  </p>
  
  <p>
    <%= f.label :designation %>
    <%= f.text_field :designation, :class => 'text' %>
  </p>
  
  <p>
    <%= f.label :photo %>
    <%= f.file_field :photo %>
  </p>

  <p>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </p>
  
  
  <%= render :partial =>"shared/button" %>

<% end %>


