

 

	<%= form_for [:admin, @press],:html => { :multipart => true } do |f| %>
	
	  
	  <%- @press.errors.full_messages.each do |message| -%>
	   <div class="errorExplanation"> <p><%= message %></p></div>
	  <%- end -%>
	   
	  
	  <div id="breadcrumb">
	  	   <%= link_to "presss", admin_presses_path %> &raquo; New
	  </div>
	 
	  <p>
	    <%= f.label :title %>
	    <%= f.text_field :title, :class => 'text' %>
	  </p>
	
	  
	  <p>
	    <%= f.label :photo %>
	    <%= f.file_field :photo%>
	  </p>

	  
	  <%= render :partial =>"shared/button" %>
	
	<% end %>


