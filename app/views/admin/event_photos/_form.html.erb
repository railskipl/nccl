



<%= form_for [:admin, :event, @event_photo],:html => { :multipart => true } do |f| %>

  
  <%- @event_photo.errors.full_messages.each do |message| -%>
   <div class="errorExplanation"> <p><%= message %></p></div>
  <%- end -%>
   
  
  <p>
    <%= f.label :photo %>
    <%= f.file_field :photo %>
    <%= f.hidden_field :event_id, :value=>@event_id %>
  </p>
  
  <%= render :partial =>"shared/button" %>

<% end %>


